# ===========================================
# FICHIER EXEMPLE - COPIER EN .env
# ===========================================
# Ne JAMAIS committer le fichier .env avec les vraies clés !

# Google Gemini AI (pour la génération des plans)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Stripe (Paiements)
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Strava API (Connexion et analyse des activités)
VITE_STRAVA_CLIENT_ID=your_strava_client_id
VITE_STRAVA_CLIENT_SECRET=your_strava_client_secret

# Firebase (Authentification et base de données)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Serveur
PORT=8080
NODE_ENV=development
APP_URL=https://your-domain.com

# Brevo (Email Marketing)
BREVO_API_KEY=xkeysib-your_brevo_api_key_here
BREVO_LIST_SUBSCRIBERS=5
BREVO_LIST_NON_SUBSCRIBERS=6
BREVO_SYNC_SECRET=your_random_secret_for_cron_sync
